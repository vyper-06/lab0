    .data
prompt: .asciz "How many litres of grains do you have\n"
result: .asciz "you have this many litres: "

    .text
    .globl _start
_start:
    # Print prompt
    la a0, prompt
    li a7, 4
    ecall

    # Read integer input
    li a7, 5
    ecall
    mv t0, a0          # store input in t0

    # Calculate result = input * 20
    li t2, 20
    div t1, t0, t2

    # Print result label
    la a0, result
    li a7, 4
    ecall

    # Print result value
    mv a0, t1
    li a7, 1
    ecall

    # Exit
    li a7, 10
    ecall

